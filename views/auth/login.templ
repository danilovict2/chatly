package auth

templ Login() {
    <div class="container">
        <h1>Login Form</h1>
        <form method="POST" class="d-flex flex-column">
            <label for="email">Email: </label>
            <input type="text" id="email" name="email">
            <label for="password">Password: </label>
            <input type="text" id="password" name="password">
            <button>Log In!</button>
        </form>
    </div>
}